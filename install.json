
{
  "resources": {
    "head": [
      {
        "type": "script",
        "src": "./app.js"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["*"],
        "execute": "MapjamWidget.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "element": {
        "title": "Location",
        "description": "Where would you like the map to appear?",
        "order": 1,
        "type": "object",
        "format": "element",
        "default": {
          "selector": "body",
          "method": "append"
        }
      },
      "map": {
        "title": "Map",
        "type": "string",
        "default": "world"
      },
      "zoom": {
        "title": "Custom Zoom",
        "type": "integer",
        "default": 15
      },
      "lat": {
        "title": "Custom Latitude",
        "type": "number"
      },
      "lng": {
        "title": "Custom Longitude",
        "type": "number"
      }
    }
  }
}
